<%= form_tag last_search_path, :remote => true do %>
	<%= hidden_field_tag "product_search_type", second_data.id %>
	<div class="form_div">
		<%= label_tag "product_search_model", "Sub-Clasificacion:" %><br/>
		<%= select_tag "product_search_model", options_from_collection_for_select(data, :id, :name) %>
	</div>
	<div class="form_div">
		<%= link_to_function "Consultar", "submitLastForm()", :id => "last_submit_link" %>
	</div>
	<div class="submit_part">
		<%= submit_tag "consultar", :id => "last_submit_button" %>
	</div>
<% end %>