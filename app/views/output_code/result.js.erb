<% if @result.count > 0 %>
	$("#error_message").hide();
	$("#success_message").show();
	$("#doc_register_id").attr('value', <%= @result.first.id %>);
	$("#folder_id").attr('value', <%= @result.first.output_folder.id %>);
<% else %>
	$("#success_message").hide();
	$("#error_message").show();
<% end %>
