<section class="output_folders_title_part">
  <hgroup class="output_folders_title">
    <h1>Nuevo Registro de Salida</h1>
  </hgroup>
</section>

<section id="output_folders_new">
	<%= render 'form' %>
</section>

<section id="output_folders_new_options">
	<%= link_to 'Volver', output_folders_path %>
</section>

<section id="client_search" class="floating_part">
	<div class="floating_body">
		<div class="title">
			<h1>Busqueda de Proveedor</h1>
			<div class="second_title">
				<%= link_to_function "X", "hideBrandsMenu(this)" %>
			</div>
		</div>
		<div class="new_body">
			<%= form_tag search_client_path, :remote => true do %>
				<div class="form_div">
					<%= label_tag "name", "Nombre"%><br/>
					<%= text_field_tag "name" %>
				</div>
				<div class="form_div">
					<%= link_to_function "Buscar", "submitRegularForm(this)" %>
				</div>
				<div class="submit_part">
					<%= submit_tag "enviar" %>
				</div>
			<% end %>
			<div id="client_search_result">

			</div>
		</div>
	</div>
</section>